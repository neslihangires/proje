<div class="row" style="padding: 50px;">
  <div class="col-12">
    <div class="card" style="display:inline-block ;">
          <% cats.forEach((element, i) => { %>


          <div class="card" style="width: 330px ; display:inline-block">
            <img class="card-img-top" src="/public/img/kedi/<%= element.catimg %>" alt="Card image cap" style="height: 175px;">
            <div class="card-body" style="height: 350px;">
              <img src="/public/img/BulutKedi.jpg" style="width: 50px">

              <p2 class="card-title"> <strong><%=element.catname %>  </strong> </p2>
              <p class="card-text"><%= element.catabout  %> </p>

              <p class="card-text"><%= element.catadress  %></p>
              <p class="card-text"><%= element.catowner  %> </p>
            </div>
            <div class="card-footer">
              <small class="text-muted"><%= element.catdate %> </small>
              <% if(Kullanicila.yetki==1){ %> 
              <button style="float:right" type="button" class="btn btn-light">
                <a href="/kedilerimiz/delete/<%= element.id %> ">KEDİYİ KALDIR</a>
              </button>
              <% } %> 

              <% if(Kullanicila.yetki==2){ %> 
                <button style="float:right" type="button" class="btn btn-light">
                  <a href="/kedilerimiz/delete/<%= element.id %> ">KEDİYİ KALDIR</a>
                </button>
                <% } %> 

            </div>
          </div>




          <% }) %>
        </div>

      </div>
    </div>
  </div>
